  "keywords": [
    "style",
    "css",
    "css-in-js"
  ],
  "bugs": {
    "url": "https://github.com/jason2506/style-script/issues"
  },
  "homepage": "https://github.com/jason2506/style-script#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "webpack": "^1.13.1"
  }
